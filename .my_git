alias g="git"
alias gst="git stash"
alias gpull="git pull"
alias gpush="git push"
alias gs="git status"
alias gc="git checkout"
alias gf="git fetch --all --prune"
alias gb="git branch"
alias gm="git merge"
alias gl="git log"
alias gr="git rebase"
alias gg="git log --oneline --decorate --all --graph"

function gp() 
{
  local str="$1"
  git status;
  git add .;
  git commit -m "$str";
  git push;
  git status;
}
