alias gg="git"
alias ggs="git status"
alias ggc="git checkout"
alias ggf="git fetch"
alias ggb="git branch"
alias ggm="git merge"
alias ggl="git log"
alias ggr="git rebase"
alias ggg="git log --oneline --decorate --all --graph"

function ggp() 
{
  local str="$1"
  git status;
  git add .;
  git commit -m "$str";
  git push;
  git status;
}
