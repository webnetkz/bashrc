alias lara="php artisan"
alias lara-new="lara new"
alias lara-migrate="lara migrate"
alias lara-dev="npm install && npm run build; composer run dev"
alias lara-about="lara about"
alias lara-config="lara config:show"
alias lara-route-list="lara route:list"
alias lara-make="lara make:"

alias lara-clear="lara route:clear;lara config:clear;lara cache:clear;lara route:cache;lara config:cache;"
